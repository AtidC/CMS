extends ../layout
block content
	// Content Header (Page header)
	section.content-header
		h1
			| Search - Scheme Rate Master
		ol.breadcrumb
			li
				a(href='#')
					i.fa.fa-dashboard
					| Home
			li.active Scheme Rate Master
	// Main content
	section.content
		// general form elements
		.box.box-primary
			// form start
			form(role='form')
				.box-body
					.form-group
						.row
							.col-sm-2
								label Scheme Rate Pattern
							// /.col
							.col-sm-4
								select.form-control
									option All
									option Act Only
									option Act+Proserm
									option Act+Refill
									option Act+Refill+Proserm
									option Device Sell-Out
									option Proserm Only
							// /.col
							.col-sm-2
								label Scheme Group
							// /.col
							.col-sm-4
								select.form-control
									option All
									option Act Only-Daily
									option Act Refill-Daily
									option Act+Proserm
									option Act+Proserm(Online)AOU1D
									option Act+Proserm(Online)AOU1D
									option Act+Proserm(Special)
									option Act_Proserm_Daily
									option Proserm Special
									option Refill AOU10D
									option Refill AOU10D200B
									option Refill AOU30D300B
									option Refill AOU7D
									option Refill+Proserm AOU10D
									option SIMJew Refill (AOU10D)
									option SIMJew Refill (AOU7D)
									option Special Refill 
									option Special Refill AOU10D
									option Special Refill AOU7D
									option USSD (Partner)
									option USSD (Post)
									option USSD (Pre)
									option USSD SIMJew
							// /.col
						// /.row
					.form-group
						.row
							.col-sm-2
								label Scheme Name
							// /.col
							.col-sm-4
								input.form-control.pull-right(type='text')
							// /.col
							.col-sm-2
								| 
							// /.col
							.col-sm-4
								| 
							// /.col
						// /.row
					.form-group
						.row
							.col-sm-2
								label Start Date 
							// /.col
							.col-sm-4
								.input-group
									.input-group-addon
										i.fa.fa-calendar
									input#reservation1.form-control.pull-right(type='text')
							// /.col
							.col-sm-2
								label End Date 
							// /.col
							.col-sm-4
								.input-group
									.input-group-addon
										i.fa.fa-calendar
									input#reservation2.form-control.pull-right(type='text')
							// /.col
						// /.row
					.form-group
						.row
							.col-sm-2
								label Aou from
							// /.col
							.col-sm-4
								input.form-control(type='text')
							// /.col
							.col-sm-2
								label Aou to
							// /.col
							.col-sm-4
								input.form-control(type='text')
							// /.col
						// /.row
					.form-group
						.row
							.col-sm-2
								label Reward Type
							// /.col
							.col-sm-4
								select.form-control
									option All
									option Cash
									option Cash &amp; DOL
									option DOL
									option Point
							// /.col
							.col-sm-2
								label Golive Status
							// /.col
							.col-sm-4
								select.form-control
									option All
									option Test
									option Golive
							// /.col
						// /.row
					.form-group
						.row
							.col-sm-2
								label Payment Frequency
							// /.col
							.col-sm-4
								select.form-control
									option All
									option Daily
									option Monthly
									option Specific
							// /.col
							.col-sm-2
								| 
							// /.col
							.col-sm-4
								| 
							// /.col
						// /.row
			// /.box-body
			.box-footer
				button.btn.btn-primary(type='button') Search
