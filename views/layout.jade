doctype html
html
	head
		meta(charset='UTF-8')
		title CMS | Console
		meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
		// bootstrap 3.0.2
		link(href='/css/bootstrap.min.css', rel='stylesheet', type='text/css')
		// font Awesome
		link(href='/css/font-awesome.min.css', rel='stylesheet', type='text/css')
		// Ionicons
		link(href='/css/ionicons.min.css', rel='stylesheet', type='text/css')
		// Morris chart
		link(href='/css/morris/morris.css', rel='stylesheet', type='text/css')
		// jvectormap
		link(href='/css/jvectormap/jquery-jvectormap-1.2.2.css', rel='stylesheet', type='text/css')
		// fullCalendar
		link(href='/css/fullcalendar/fullcalendar.css', rel='stylesheet', type='text/css')
		// Daterange picker
		link(href='/css/daterangepicker/daterangepicker-bs3.css', rel='stylesheet', type='text/css')
		// bootstrap wysihtml5 - text editor
		link(href='/css/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css', rel='stylesheet', type='text/css')
		// iCheck for checkboxes and radio inputs
		link(href='/css/iCheck/all.css', rel='stylesheet', type='text/css')
		// Bootstrap Color Picker
		link(href='/css/colorpicker/bootstrap-colorpicker.min.css', rel='stylesheet')
		// Bootstrap time Picker
		link(href='/css/timepicker/bootstrap-timepicker.min.css', rel='stylesheet')
		// DATA TABLES
		link(href='/css/datatables/dataTables.bootstrap.css', rel='stylesheet', type='text/css')
		// demo style for icons
		link(href='/css/AdminLTE/icons.css', rel='stylesheet', type='text/css')
		// Ion Slider
		link(href='/css/ionslider/ion.rangeSlider.css', rel='stylesheet', type='text/css')
		// ion slider Nice
		link(href='/css/ionslider/ion.rangeSlider.skinNice.css', rel='stylesheet', type='text/css')
		// bootstrap slider
		link(href='/css/bootstrap-slider/slider.css', rel='stylesheet', type='text/css')
		// Theme style
		link(href='/css/AdminLTE.css', rel='stylesheet', type='text/css')
		// HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
		// WARNING: Respond.js doesn't work if you view the page via file://
		//if lt IE 9
			script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
			script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
	body.skin-blue
		// header logo: style can be found in header.less
		header.header
			a.logo(href='/index.html')
				// Add the class icon to your logo image or logo icon to add the margining
				| CMS
				small
					|  - Console
			// Header Navbar: style can be found in header.less
			nav.navbar.navbar-static-top(role='navigation')
				// Sidebar toggle button
				a.navbar-btn.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				.navbar-right
					ul.nav.navbar-nav
			
						// User Account: style can be found in dropdown.less
						li.dropdown.user.user-menu
							a.dropdown-toggle(href='#', data-toggle='dropdown')
								i.glyphicon.glyphicon-user
								span
									| Nichanan Chaiton
									i.caret
							ul.dropdown-menu
								// User image
								li.user-header.bg-light-blue
									img.img-circle(src='/img/avatar3.png', alt='User Image')
									p
										| Nichanan Chaiton 
										small Partner/Sales Code : 71000155
								// Menu Footer
								li.user-footer
									.pull-left
										h5.text-blue Incentive Team (R2) 
									.pull-right
										a.btn.btn-default.btn-flat(href='#') Sign out
		.wrapper.row-offcanvas.row-offcanvas-left
			// Left side column. contains the logo and sidebar
			aside.left-side.sidebar-offcanvas
				// sidebar: style can be found in sidebar.less
				section.sidebar
					// Sidebar user panel
					.user-panel
						.pull-left.image
							img.img-circle(src='/img/avatar3.png', alt='User Image')
						.pull-left.info
							p Hello, Nichanan
							a(href='#')
								| Incentive Team (R2)

					// search form
					form.sidebar-form(action='#', method='get')
						.input-group
							input.form-control(type='text', name='q', placeholder='Search...')
							span.input-group-btn
								button#search-btn.btn.btn-flat(type='submit', name='seach')
									i.fa.fa-search
					// /.search form
					// sidebar menu: : style can be found in sidebar.less
					ul.sidebar-menu
						li
							a(href='/console/index.html')
								i.fa.fa-dashboard
								span Dashboard
						li
							a(href='/console/userData.html')
								i.fa.fa-user
								span User Data
	
						li.treeview.active 
							a(href='#')
								i.fa.fa-bar-chart-o
								span Scheme Rate Setting
								i.fa.fa-angle-left.pull-right
							ul.treeview-menu
								li
									a(href='/console/schemeRateSearch2.html')
										i.fa.fa-search
										small Search Scheme Rate Setting 
								li
									a(href='/console/schemeRateAdd.html')
										i.fa.fa-plus-circle
										small Add Scheme Rate Setting
						li.treeview
							a(href='#')
								i.fa.fa-gavel
								span Scheme Rule Setting
								i.fa.fa-angle-left.pull-right
							ul.treeview-menu
								li
									a(href='/console/schemeRuleSearch2.html')
										i.fa.fa-search
										small Search Scheme Rule Setting 
								li
									a(href='/console/schemeRuleAdd.html')
										i.fa.fa-plus-circle
										small Add Scheme Rule Setting
						li.treeview
							a(href='#')
								i.fa.fa-dollar
								span Monthy Commission
								i.fa.fa-angle-left.pull-right
							ul.treeview-menu
								li
									a(href='/console/comoth.html')
										i.fa.fa-angle-double-right
										small Commission Other Type
								li
									a(href='/console/comadj.html')
										i.fa.fa-angle-double-right
										small Commission Adjust Data 
								li
									a(href='/console/concom.html')
										i.fa.fa-angle-double-right
										small Confirm Commission
								li
									a(href='/console/comsum.html')
										i.fa.fa-angle-double-right
										small Commission Summary
								li
									a(href='/bkrej.html')
										i.fa.fa-angle-double-right
										small Bank Reject Data
						li.treeview
							a(href='#')
								i.fa.fa-reply-all
								span Export Data File
								i.fa.fa-angle-left.pull-right
							ul.treeview-menu
								li
									a(href='/exfile.html')
										i.fa.fa-reply
										| Export Data File
								li
									a(href='/dwfile.html')
										i.fa.fa-download
										| Download Export Data File
						
				// /.sidebar
			// Right side column. Contains the navbar and content of the page
			aside.right-side
				block content
			// /.right-side
		// ./wrapper
		block message-modal
		block javascript
